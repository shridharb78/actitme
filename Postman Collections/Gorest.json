	"item": [
		{
			"name": "Create USer",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "928e995c-d46e-4b13-85eb-82ef7466ab30",
						"exec": [
							"var random=Math.random().toString(36).substring(2);\r",
							"var name=\"shri\"+random;\r",
							"var email=\"shri\"+random+\"@gmail.com\";\r",
							"pm.environment.set(\"name_env\",name);\r",
							"pm.environment.set(\"email_env\",email);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "5eb23cef-12cf-401e-9b8f-f5e5b04a5496",
						"exec": [
							"var jsonData=JSON.parse(responseBody);\r",
							"\r",
							"pm.environment.set(\"id\",jsonData.id);\r",
							"pm.test(\"Status code validation\",()=>\r",
							"{\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Header validation\",()=>\r",
							"{\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.eql(\"application/json; charset=utf-8\");\r",
							"    pm.response.to.have.header(\"Connection\");\r",
							"\r",
							"    pm.expect(pm.response.headers.get(\"Connection\")).to.eql(\"keep-alive\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "b5028885-284d-49ce-a385-c79d4b4641b8",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\"name\":\"{{name_env}}\",\r\n \"gender\":\"male\", \r\n\"email\":\"{{email_env}}\", \r\n\"status\":\"active\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "{{baseUrl_collection}}/public/v2/users"
			},
			"response": []
		},
		{
